<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent">
<Button
    android:id="@+id/start"
    android:layout_height="wrap_content"
    android:text="Add VPN" android:layout_width="wrap_content">
	</Button>
<TextView 	android:background="#333333" 
			android:text="VPNs" 
			android:id="@+id/textView1" 
			android:layout_width="match_parent" 
			android:layout_height="wrap_content" /> 
<ListView android:id="@+id/listView1" android:layout_height="wrap_content" android:layout_width="match_parent" />
<TableLayout android:id="@+id/tableLayout1" android:layout_width="match_parent" android:layout_height="wrap_content">
</TableLayout>
</LinearLayout>
